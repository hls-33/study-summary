$("input[name='aphone']").blur(function(){
	$.ajax({
		url:"/bikesystem/aphone",
		type:"post",
		date:{"aphone":$("input[name='aphone']").val()},
		success:function(message){
			$("#aphonespan").html(message);
		}
	});
});
