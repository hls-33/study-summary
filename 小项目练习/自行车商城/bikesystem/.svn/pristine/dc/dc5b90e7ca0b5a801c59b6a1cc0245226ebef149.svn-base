@CHARSET "UTF-8";
#workspace{
	display: block;
	width:100%;
	height:60px;
	font-size:24px;
	font-weight: bolder;
	line-height: 60px;
	color:rgb(0,176,240);
	border-bottom: 2px solid gray;
	background-color: rgb(196,242,234);
}
#workspace span{
	background-color: rgb(196,242,234);
	display: inline-block;
	width:20%;
	line-height: 60px;
}
#workspace span:nth-of-type(1){
	float:left;
	text-align: right;
}
#workspace span:nth-of-type(2){
	float:right;
}
#workspace span:nth-of-type(2) img{
	position: relative;
	top:20px;
}