		super();
		this.uesrname = uesrname;
		this.address = address;
		this.minuteaddress = minuteaddress;
		this.phone = phone;
	}
	
	public SellAddress(int said, String name, String uesrname, String address, String minuteaddress, String phone) {
		super();
		this.said = said;
		this.name = name;
		this.uesrname = uesrname;
		this.address = address;
		this.minuteaddress = minuteaddress;
		this.phone = phone;
	}

	public SellAddress(String name, String uesrname, String address, String minuteaddress, String phone) {
		super();
		this.name = name;
		this.uesrname = uesrname;
		this.address = address;
		this.minuteaddress = minuteaddress;
		this.phone = phone;
	}

	public SellAddress(int said, String uesrname, String address, String minuteaddress, String phone) {
		super();
		this.said = said;
		this.uesrname = uesrname;
		this.address = address;
		this.minuteaddress = minuteaddress;
		this.phone = phone;
	}
	public int getSaid() {
		return said;
	}
	public void setSaid(int said) {
		this.said = said;
	}
	public String getUesrname() {
