@CHARSET "UTF-8";


.shop-bikes{
	margin-top:30px;
	margin-left:30px;
}

.kind-nav{
	width:150px;
	border:1px solid rgb(221, 221, 221);
	float:left;
}

.kind-nav-title{
	height: 36px;
	padding-left: 20px;
}
.kind-nav-rent{
	height: 72px;
	padding-left: 20px;
}
.kind-nav-rent a:first-child{
	font-weight: bolder;
	display: block;
}
.kind-nav-rent a:NTH-OF-TYPE(2){
	display: inline-block;
	margin-left:10px;
}
.kind-nav-rent a:NTH-OF-TYPE(3){
	display: inline-block;
}

.kind-nav-sell{
	height: 72px;
	padding-left: 20px;
}
.kind-nav-sell a:NTH-OF-TYPE(2){
	display: inline-block;
	margin-left:10px;
}
.kind-nav-sell a:NTH-OF-TYPE(3){
	display: inline-block;
}

.kind-nav-kindname{
	height: 36px;
	padding-left: 20px;
}
