package com.bikesystem.entity;

import java.math.BigDecimal;

/**
 * 商家租车优惠记录的实体类
 * 对应shop_rent_sale表
 * @author bwfadmin
 *
 */
public class ShopRentSale {
	private int srid;
	private int brid;
	private String shopName;
	private BigDecimal hourSalePrice;
	private BigDecimal daySalePrice;
	
	public ShopRentSale() {
		super();
	}
	public ShopRentSale(int srid, int brid, String shopName, BigDecimal hourSalePrice, BigDecimal daySalePrice) {
		super();
		this.srid = srid;
		this.brid = brid;
		this.shopName = shopName;
		this.hourSalePrice = hourSalePrice;
		this.daySalePrice = daySalePrice;
	}
	
	public ShopRentSale(int brid, String shopName, BigDecimal hourSalePrice, BigDecimal daySalePrice) {
		super();
		this.brid = brid;
		this.shopName = shopName;
		this.hourSalePrice = hourSalePrice;
		this.daySalePrice = daySalePrice;
	}
	public int getSrid() {
		return srid;
	}
	public void setSrid(int srid) {
		this.srid = srid;
	}
	public int getBrid() {
		return brid;
	}
	public void setBrid(int brid) {
		this.brid = brid;
	}
	public String getShopName() {
		return shopName;
	}
	public void setShopName(String shopName) {
		this.shopName = shopName;
	}
	public BigDecimal getHourSalePrice() {
		return hourSalePrice;
	}
	public void setHourSalePrice(BigDecimal hourSalePrice) {
		this.hourSalePrice = hourSalePrice;
	}
	public BigDecimal getDaySalePrice() {
		return daySalePrice;
	}
	public void setDaySalePrice(BigDecimal daySalePrice) {
		this.daySalePrice = daySalePrice;
	}
	@Override
	public String toString() {
		return "ShopRentSale [srid=" + srid + ", brid=" + brid + ", shopName=" + shopName + ", hourSalePrice="
				+ hourSalePrice + ", daySalePrice=" + daySalePrice + "]";
	}
	
	
	
}
