@CHARSET "UTF-8";
table{
	width:100%;
}
.good_div td,th{
	border:1px solid #E0E0E0;
	text-align:center;
	
}
.good_div img{
	width:80px;
	height:80px;
}
.gooddescription{
	width:100%;
	height:100%;
}
.good_div td:nth-of-type(4){
	width:90px;
}
.good_div td:nth-of-type(1),.good_div td:nth-of-type(2),.good_div td:nth-of-type(3){
	width:202px;
}
#content_table{
	background-color:#F1F1F1;
}
.good_div td{
	font:14px 黑体;
	color:#3C3C3C;
}
td[colspan="2"]{
	text-align:left;
}
.good_div tr:nth-of-type(1)>td:nth-of-type(1){
	border-right:1px solid white;
	width:180px;
}

.good_div td:nth-of-type(2){
	width:265px;
	text-align:left;
}
.good_div td:nth-of-type(3),.good_div td:nth-of-type(4){
	width:100px;
}
.good_div td:nth-of-type(5){
	width:110px
}
.content_div{
	border:1px solid white;
}
#content_table th:nth-of-type(1){
	width:438px;
}
#content_table th:nth-of-type(2){
	width:90px;
}
#content_table th:nth-of-type(3){
	width:90px;
}
#content_table th:nth-of-type(4){
	width:100px;
}
.updatebut{
	border:1px solid #FF4401;
	background-color:#FFFFFF;
	color:black;
	border-radius:3px;
	width:40px;
	height:25px;
	font:14px;
	
}
.updatebut:hover{
	background:#FF4401;
	color:#FFFFFF;
}
















