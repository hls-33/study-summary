@CHARSET "UTF-8";
a{
	text-decoration:none;
	color:#666666;
}
a:hover{
	text-decoration:underline;
}
li{
	list-style:none;
}
table{
	border-collapse:collapse;
}
.one_font{
	font:16px 黑体;
	color:black;
	font-weight:bold;
}
.two_font{
	font:14px 黑体;
	color:#666666;
}
.top_div{
	width:100%;
	height:40px;
	background-color:#F6F6F6;
	margin-top:-8px;
	
}
.topcontent_div{
	width:90%;
	height:100%;
	margin:auto;
	background-color:skyblue;
}
.background_div{
	width:90%;
	height:1000px;
	margin:auto;
}
.tittle_div{
	width:100%;
	height:151px;
	float:left;
}
.top_navigation_div{
	width:100%;
	height:50px;
	background-color:#0085D7;
	float:left;
}
.top_navigation_div ul{
	margin:0;
	padding:0;
}
.top_navigation_div li:nth-of-type(1){
	margin-left:20px;
}
.top_navigation_div li{
	font:15px 黑体;
	font-weight:bold;
	color:white;
	float:left;
	width:50px;
	height:100%;
	list-style-type:none;
	text-align:center;
	line-height:50px;
	margin-right:30px;
}
.top_navigation_div li:hover{
	color:#C7E4F6;
	cursor:pointer;
}
.navigation_div{
	width:18%;
	height:480px;
	margin-top:20px;
	border:1px solid #E0E0E0;
	float:left;
}
.navigation_div ul{
	margin:0;
	padding:0;
}
.content_div{
	width:80%;
	height:480px;
	margin-left:20px;
	margin-top:20px;
	border:1px solid #E0E0E0;
	float:left;
}
.tittle_div>img{
	width:210px;
	height:50%;
	margin-top:33px;
}
#rent{
	margin:0;
	padding:0;
}
.function_menu_div>ul>li{
	margin:auto;
	width:85%;
	border-bottom:1px solid #E0E0E0;
	padding-top:13px;
	padding-bottom:13px;
}
.function_ul li{
	border:1px solid black;
	width:90px;
}
