@CHARSET "UTF-8";

*{
	margin: 0px;
	padding:0px;
}

body{
	font:12px/1.5 tahoma,arial,'Hiragino Sans GB',\5b8b\4f53,sans-serif;
	font-family:arial,'Hiragino Sans GB',"\5b8b\4f53",sans-serif;
}
a{
	text-decoration: none;
}
ul li{
	list-style: none;
}
/* 搜索主体main start */
.main{
	width:940px;
}
	/* 搜索框 */
.header-wraper{
	width:940px;
	height:44px;
}
.logo{
	float:left;
	width:105px;
	height:43px;
}
.logo img{
	width:105px;
	height:43px;
}
.search{
	width:681px;
	float:right;
	margin-top:4px;
	height:38px;
}
.wraper{
	float:left;
	border:solid 2px #f50;
	border-right:none;
	height:35px;
}
.tab-wraper{
	position:relative;
	display:inline-block;
	float:left;
	width:67px;
	height:31px;
	
}
.tab{
	position:absolute;
	top:0px;
	left:0px;
	width:66px;
	overflow:hidden;
	height:31px;
	line-height: 31px;
    border-right: solid 1px #e5e5e5;
    border-left: solid 1px #f5f5f5;
	background:#f5f5f5;
}
.triggers{
	display:inline-block;
	width:66px;
	text-indent: 15px;
    color: #6b6b6b;
}
.arrow{
	display:inline-block;
	position:relative;
	background-position: -296px -423px;
	left:50px;
	top:-58px;
    width: 7px;
    height: 4px;
	background-image:url("/bikesystem/image/hs/淘宝资源图.png");
}
.tab-hover{
	z-index: 1000;
	height:62px;
	background:#fff;
	border:1px solid #e5e5e5;
	border-top:none;
}
.triggers>li:hover{
	background:#f5f5f5;
}
.submit{
	float:left;
	width:80px;
	height:35px;
	border:none;
	cursor:pointer;
	background-position:-125px -58px;
	display:inline-block;
	font-size:0px;
	background-image:url("/bikesystem/image/hs/淘宝资源图.png");
	background-repeat:no-repeat;
}
.submit li{
	background: #f5f5f5;
}
.input-search{
	display:inline-block;
}
.search-combobox{
	vertical-align: middle;
    position: relative;
    font-size: 12px;
}
.search-input{
	width:530px;
	height: 30px;
    border: none 0;
    outline: 0;
    font-size: 14px;
    padding-left: 5px;
}
	/* end */

	/* 自行车种类导航栏 */
#mainsrp-nav{
	
}
.m-nav{
	margin-top:10px;
	font-weight:400;
	border:1px solid rgb(232,232,232);
}
.crumb{
	margin-left:20px;
	margin-top:10px;
}
.h{
	color:#F40!important;
}
.group{
	margin:0 8px;
	border-bottom:1px dashed #dedede;
	height:37px;
}
.nav-title{
	display:inline-block;
	width:100px;
	color:#999;
	margin-top:10px;
}
.nav-text{
	display:inline-block;
	margin-right: 15px;
	margin-top:10px;
}
	/* end */
/* end */

/* 搜索结果框 */
.s-result{
	margin-top:20px;
}
	/* 单个搜索结果框 */
.result-item{
	width:220px;
	height:315px;
	margin-right:20px;
	float:left;
	border:1px solid #ededed;
}
.result-item:nth-of-type(4){
	margin-right:0px;
}
	/* end */
	/* 图片显示框 */
.img-item{
	width:220px;
	height:220px;
}
.img-item img{
	width:220px;
	height:220px;
}
	/* end */
	/* 租金价格 */
.price-item{
	margin-top:11px;
	width:220px;
	height:22px;
	font-family:arial;
	font-size:18px;
	line-height:22px;
	overflow:hidden;
	padding:0 10px;
}	
.price{
	float:left;
	color:#F40;
	width:120px;
}
.price .span1{
	margin-right: 3px;
}
.price strong{
	font-family:arial;
	letter-spacing:0.8px;
}
.price .span2{
	display: inline-block;
	background-color:#F40;
	height:18px;
	margin-left:2px;
	color:white;
	font-size:10px;
}
.choose-count{
	float:right;
	padding-left:15px;
	width:85px;
	color:#888;
	line-height:22px;
	height:22px;
	font-size:12px;
}
	/* end */
	/* 自行车商品名 */
.goods-title-item{
	margin-top:6px;
	height:24px;
	padding:0 10px;
	overflow:hidden;
	font-weight:400;
}
.goods-title-item .span3{
	display: inline-block;
	float:right;
	color:#888;
	line-height:14px;
	height:14px;
	font-size:12px;
}
	/* end */
	/* 商店名 */
.shop-item{
	margin-top:5px;
	height:22px;
	line-height:22px;
	overflow:hidden;
	padding:0 10px;
}
.shopname{
	color:#888;
	text-decoration:underline;
}
.dsrs{
	margin-top:4px;
	width:13px;
	float:left;
}
.dsr{
	float:left;
	margin-top:2px;
	height:2px;
	width:8px;
	overflow:hidden;
}
.equalthan{
	background:#f40;
}



