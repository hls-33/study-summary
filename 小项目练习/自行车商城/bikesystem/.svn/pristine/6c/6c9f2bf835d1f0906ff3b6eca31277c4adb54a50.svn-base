@CHARSET "UTF-8";
*{
	margin: 0;
	padding: 0;
	font:12px/1.5 tahoma,arial,'Hiragino Sans GB',\5b8b\4f53,sans-serif;
	font-family:arial,'Hiragino Sans GB',"\5b8b\4f53",sans-serif;
}
#top-div{
	width:90%;
	margin: auto;
	margin-top:10px; 
	position: relative;
	background-color: rgb(254,242,204);
}
#top-menu-list,#top-menu-list2{
	display: block;
	float:right;
}
li{
	width:200px;
	display: inline-block;
	height:40px;
	text-align:center;
	font-size:18px;
	list-style-type: none;
	line-height: 40px;
}
#top-menu-list li{
	color:red;
}
#top-menu-list li:hover{
	color:blue;
	cursor: pointer;
}
#top-menu-list2{
	position:absolute;
	bottom:0px;
	right:60px;
	color:rgb(0,176,240);
}
#top-menu-list2 li:hover{
	color:blue;
	cursor: pointer;
}


#show-div{
	width:90%;
	height:600px;
	margin: auto;
}
#list-div,#info-div{
	height:100%;
	float:left;
	border:1px solid gray;
	background-color: rgb(196,242,234);
}
#list-div{
	width:30%;
}
#info-div{
	margin-left: 4%;
	width:60%;
}
#introduce{
	display: block;
	width:100%;
	height:60px;
	font-size:24px;
	font-weight: bolder;
	line-height: 60px;
	text-align: center;
	color:rgb(0,176,240);
	border-bottom: 2px solid gray;
	background-color: rgb(196,242,234);
}
#adminmanger{
	width:70%;
	margin: auto;
	margin-top: 10px;
}
#adminmanger li{
	width:100%;
	font-size:16px;
	display:block;
	line-height:30px;
	background-color: rgb(196,242,234);
	text-align: left;
	border-bottom: 1px dotted gray;
	list-style-type: none;
}
。infomanner{
	width:100%;
}
。infomanner p{
	width:100%;
	font-size:14px;
	display:inline-block;
	height:40px;
	line-height:40px;
	text-align: right;
}
.pre{
	height:90px;
}

#bottom-div{
	width:100%;
	height:100px;
	text-align: center;
	position:absolute;
	top:690px;
	z-index:2;
	background-color: rgb(197,219,194);
}
#bottom-div ul{
	list-style: none;
	width:100%;
	text-align: center;
	background-color: rgb(197,219,194);
}
#bottom-div ul li{
	display: inline-block;
	margin-top:5px;
	width:110px;
	text-align: center;
	line-height: 40px;
}
#bottom-div ul li a{
	text-decoration: none;
	color:gray;
	font-family: "宋体";
}
#bottom-div ul li:hover{
	background-color: rgb(209,209,175);
}
#bottom-div ul li:hover a{
	color:black;
	background-color: rgb(209,209,175);
}
#bottom-div p{
	color:gray;
	font-family: "宋体";
	background-color: rgb(197,219,194);
	font-size:14px;
	line-height: 20px;
}


/*adminmannerinfo-----css*/

#info-div div{
	width:100%;
	height:100%;
	display: none;
}

#selpersoninfo{
	background-color: rgb(196,242,234);
}
.mannerintro{
	display: inline-block;
	width:100%;
	font-size:24px;
	color:rgb(0,176,240);
	text-align: center;
	line-height: 60px;
	height:60px;
	background-color: rgb(196,242,234);
	border-bottom: 2px solid gray;
}
#info_person{
	width:70%;
	margin: auto;
}
#info_person table{
	width:100%;
	border-collapse: collapse;
}
#info_person table td{
	border-bottom:1px solid gray;
	width:33%;
	height:60px;
}


/*userpersoninfo--css*/
#usermanger{
	width:80%;
	margin: auto;
	margin-top: 30px;
}

#usermanger li{
	height:50px;
	list-style-type: none;
	width:100%;
	text-align: left;
	background-color: rgb(196,242,234);
	border-bottom: 1px dotted gray;
	
}
#usermanger li p{
	height:50px;
	display: inline-block;
	background-color: rgb(196,242,234);
}










